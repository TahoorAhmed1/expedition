{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "consistent-return": "off",
    "max-len": "off",
    "no-param-reassign": "off",
    "no-underscore-dangle": "off"
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [["@", "."]],
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
